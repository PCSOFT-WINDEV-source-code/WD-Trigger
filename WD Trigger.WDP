      GLOBAL
      	gsRights is string		// Rights of the user
      	gbTrace is boolean		// Displays the trace window
      LOCAL
      	
      gsRights = “L”
      gbTrace = True
      
      // HF
      HMode(hModeMulti)
      HCreationIfNotFound(“*”)
      
      // Create the triggers
      // 'Before' Trigger
      HDescribeTrigger(CHARGE, “HAdd,HModify,HDelete”, Trigger_Proc, hTriggerBefore)
      
      // 'After' trigger
      HDescribeTrigger(CHARGE, “HAdd,HModify,HDelete”, Trigger_Proc, hTriggerAfter)
      
      
      CASE Error:
      Error(<§@00000000000000000003§>,HErrorInfo())
